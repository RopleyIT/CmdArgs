<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
	  <Version>1.0.1</Version>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>RopleyIT.$(AssemblyName)</PackageId>
    <Authors>RopleyIT</Authors>
    <Company>Ropley IT Ltd</Company>
    <Description>Another simple command line argument parser</Description>
    <Copyright>Ropley Information Technology Ltd</Copyright>
    <PackageProjectUrl>https://github.com/RopleyIT/CmdArgs</PackageProjectUrl>
  </PropertyGroup>
</Project>
